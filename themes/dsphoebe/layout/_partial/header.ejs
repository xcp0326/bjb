<header class="header">
  <nav class="navbar">
    <ul class="menu">
      <% for (name in theme.menu) { %>
        <li class="menu-item">
          <a href="<%- url_for(theme.menu[name]) %>" class="menu-item-link"><%- __('Menu.' + name) %></a>
        </li>
      <% } %>
    </ul>
  </nav>
  <h1 class="pc"><%= config.pc_title %></h1>
  <h1 class="mobile"><a href="/"><%= config.tabtitle %></a></h1>
  <em><%= config.subtitle %></em>
</header>